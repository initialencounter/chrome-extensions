{
  "name": "lims",
  "version": "1.0.0",
  "description": "- 下载 lims 文件夹内所有文件。",
  "main": "hotkey.js",
  "scripts": {
    "dev": "webpack --watch --mode development",
    "build": "vite build && npm run copy-files",
    "copy-files": "powershell -Command \"Copy-Item -Path 'src/manifest.json' -Destination 'dist/manifest.json'; Copy-Item -Path 'src/assets' -Destination 'dist/assets' -Recurse\"",
    "zip": "7z a -tzip ./%npm_package_name%-%npm_package_version%.zip -r dist/*",
    "lint": "eslint src/ts/*.ts",
    "format": "prettier --write src/ts/*.ts"
  },
  "keywords": [],
  "license": "AGPL-3.0",
  "author": {
    "name": "initencounter",
    "email": "2911583893@qq.com",
    "url": "https://github.com/initialencounter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/initialencounter/chrome-extensions.git"
  },
  "bugs": {
    "url": "https://github.com/initialencounter/chrome-extensions/issues"
  },
  "homepage": "https://github.com/initialencounter/chrome-extensions/blob/master/lims/readme.md#使用方法",
  "devDependencies": {
    "@types/chrome": "^0.0.270",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.35.0",
    "prettier": "^3.3.3",
    "terser": "^5.31.6",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-static-copy": "^1.0.6"
  },
  "packageManager": "yarn@1.22.22"
}
